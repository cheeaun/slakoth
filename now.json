{
  "alias": "slakoth",
  "version": 2,
  "regions": ["sin"],
  "env": {
    "SLACK_TOKEN": "@slack-bot-token",
    "SLACK_CHANNEL": "@slack-channel"
  },
  "builds": [
    { "src": "server.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/favicon.ico", "status": 204 },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}